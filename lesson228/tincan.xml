  <interactionType>choice</interactionType>
  <correctResponsePatterns>
    <correctResponsePattern>choice_cm3ek8ooj001p2a6w05sn62hh</correctResponsePattern>
    </correctResponsePatterns>
    <choices>
    <component>
    <id>choice_cm3ek8ooj001o2a6w8yvytsbh</id>
    <description lang="en-US">&lt;div data-editor-id=&#34;e1475301-92a4-4993-a0f2-073260eb9a18&#34;&gt;Die statische Länge entspricht dem Lichtmaß zwischen den beiden Wänden&lt;/div&gt;</description>
  </component>
    <component>
    <id>choice_cm3ek8ooj001p2a6w05sn62hh</id>
    <description lang="en-US">&lt;div data-editor-id=&#34;693d210a-9846-4b8d-8a6c-3f2e0a64f702&#34;&gt;Die statische Länge wird von Wandmitte zur Wandmitte angenommen.&lt;/div&gt;</description>
  </component>
    <component>
    <id>choice_cm3ek8opq001q2a6wl1arkx9f</id>
    <description lang="en-US">&lt;div data-editor-id=&#34;164734a0-c0be-42f6-bfa4-c75b5f5e327e&#34;&gt;Die statische Länge wird von Außenwand zu Außenwand angenommen.&lt;/div&gt;</description>
  </component>
    <component>
    <id>choice_cm3ek93b0001s2a6wvenngmqu</id>
    <description lang="en-US">&lt;div data-editor-id=&#34;9e39b058-df8a-4ba8-b491-3f678520f47b&#34;&gt;Die statische Länge entspricht dem Lichtmaß + 10 cm&lt;/div&gt;</description>
  </component>
    </choices>
  </activity>
                              
