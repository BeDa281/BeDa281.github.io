    <activity id="http://I_mjwZPugZTHzEVPT5-jkkgmncYYnFpS_rise/FlEJM3kwSqBMRRGE-R3z7kxWF4hapwDC/cl13f2r9x000j266f6cndws2t" type="http://adlnet.gov/expapi/activities/cmi.interaction">
      <name lang="en-US">Wer ver&amp;ouml;ffentlicht die Bauregellisten (BRL)?</name>
