            <component>
        <id>choice_cl0wdbwrh0027266f31x736u6</id>
        <description lang="en-US">Am 25. Juni</description>
      </component>
            <component>
        <id>choice_cl0wdbwrh0028266f51uzwwu2</id>
        <description lang="en-US">Am 31. Juli</description>
